{
  "manifest_version": 3,
  "name": "Goose Nest Chrome",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": [
    "*://*.facebook.com/marketplace/*"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "src/popup.html"
  }
}
