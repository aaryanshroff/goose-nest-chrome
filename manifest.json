{
  "manifest_version": 3,
  "minimum_chrome_version": "93",
  "name": "Goose Nest Chrome",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": [
    "*://*.facebook.com/marketplace/*"
  ],
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "dist/popup.html"
  }
}
